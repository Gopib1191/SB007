<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletExample</display-name>
  
  <welcome-file-list>
    <welcome-file>onlinetraining.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  		<description>Student Registration Form Servlet</description>
  		<display-name>OnlinetrainingServlet</display-name>
  		<servlet-name>OnlinetrainingServlet</servlet-name>
  		<servlet-class>com.simplilearn.servlet.OnlinetrainingServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>OnlinetrainingServlet</servlet-name>
  		<url-pattern>/hire-from-us</url-pattern>
  	</servlet-mapping>
  	
</web-app>